<form class="form-horizontal" role="form" method="post" action="/sifut/lapang/edit/<%=data[0].id_lapang%>">
  <div class="form-group">
    <label for="nama" class="col-sm-2 control-label">id lapang</label>
    <div class="col-sm-10">
      <input type="text" name="id_lapang" value="<%=data[0].id_lapang%>" class="form-control" placeholder="id lapang">
    </div>
  </div>
  <div class="form-group">
    <label for="email" class="col-sm-2 control-label">nama</label>
    <div class="col-sm-10">
      <input type="text" name="nama" value="<%=data[0].nama%>" class="form-control"  placeholder="nama">
    </div>
  </div>
  <div class="form-group">
    <label for="pesan" class="col-sm-2 control-label">aktif</label>
    <div class="col-sm-10">
      <input type="text" name="aktif" value="<%=data[0].aktif%>" class="form-control"  placeholder="aktif">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <input type="submit" value="Save">
    </div>
  </div>

</form>
